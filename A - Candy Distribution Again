#include <stdio.h>
#include <stdlib.h>

long long sum_long_long_list(const void *a, int num){
  long long sum=0;
  long long *A = (long long*)a;
  for(int i=0; i<num; i++){
    sum += *A;
    A++;
  }
  return sum;
}

int cmp_long_long(const void *a, const void *b){
  long long x = *(long long *)a;
  long long y = *(long long *)b;
  
  if(x-y<0) return -1;
  else if(x-y==0) return 0;
  else return 1;
}

int main(void){
  int N;
  long long x, a[105], num_snacks=0;
  scanf("%d %lld", &N, &x);
  for(int i=0; i<N; i++){
    scanf("%lld", &a[i]);
  }
  
  qsort(a, N, sizeof(long long), cmp_long_long);
  num_snacks = sum_long_long_list(a, N);
  if(num_snacks == x) printf("%d", N);
  else if(num_snacks < x){
    printf("%d", N-1);
  }
  
  else{
    num_snacks = 0;
    for(int j=0; j<N; j++){
      num_snacks += a[j];
      if(num_snacks > x){
        printf("%d", j);
        break;
      }
      else if(num_snacks == x){
        printf("%d", j+1);
        break;
      }
    }
  }
  return 0;
}
