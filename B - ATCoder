#include <stdio.h>
#include <string.h>

int main(void){
  char a[15], *A;
  int n=0, max=0;
  
  scanf("%s", a);
  A = a;
  
  for(int i=0; i<strlen(a); i++){
    if(*(A+i)=='A'||*(A+i)=='C'||*(A+i)=='G'||*(A+i)=='T'){
      n+=1;
      if(max<n) max = n;
    }
    else n = 0;
  }
  
  printf("%d", max);
  return 0;  
}
