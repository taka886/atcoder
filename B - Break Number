#include <stdio.h>

int main(void){
  int N, num_dv=0, j, M=0, num_max=1;
  scanf("%d", &N);
  for(int i=1; i<=N; i++){
    j = i;
    num_dv = 0;
    while(j%2==0){
      j /= 2;
      num_dv += 1;
    }
    if(num_dv > M){
      M = num_dv;
      num_max = i;
    }
  }
  printf("%d", num_max);
  return 0;
}
