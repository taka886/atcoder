#include <stdio.h>

int main(void){
  int N, M, C, B[25], A[25], p=0, sum=0;
  scanf("%d %d %d", &N, &M, &C);
  for(int i=0; i<M; i++){
    scanf("%d", &B[i]);
  }
  for(int s=0; s<N; s++){
    for(int j=0; j<M; j++){
      scanf("%d", &A[j]);
      sum += A[j] * B[j];
    }
    if(sum+C > 0) {
      p++;
    }
    sum = 0;
  }
  printf("%d", p);
  return 0;
}
