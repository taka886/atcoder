#include <stdio.h>

int main(void){
  int N, A, B, num_b=0, num_acp=0; 
  char s[100005];
  scanf("%d %d %d %s", &N, &A, &B, s);
  for(int i=0; i<N; i++){
    if(num_acp<A+B&&s[i] == 'a'){
      printf("%s\n", "Yes");
      num_acp++;
    }
    else if(num_acp<A+B&&s[i] == 'b'&&num_b<B){
      printf("%s\n", "Yes");
      num_b ++;
      num_acp++;
    }
    else printf("%s\n", "No");
  }
  return 0;
}
