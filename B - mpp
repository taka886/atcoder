#include <stdio.h>
#include <string.h>

int main(void){
  int N, a[30]={0}, max=0, t;
  char s[105];
  scanf("%s", s);
  N = strlen(s);
  for(int i=0; i<N; i++){
    a[s[i] - 'a'] += 1;
    t = a[s[i] - 'a'];
    if(max < t) max = t;
  }
  for(int i=0; i<N; i++){
    t = a[s[i] - 'a'];
    if(t != max) printf("%c", s[i]);
  }
  return 0;
}
