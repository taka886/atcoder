#include <stdio.h>

  long long win[100005]={0};
int main(void){
  long long N, K, Q, t;
  scanf("%lld %lld %lld", &N, &K, &Q);
  
  for(int i=0; i<Q; i++){
    scanf("%lld", &t);
    win[t-1] += 1;
  }
  for(int i=0; i<N; i++){
    if(K - ( Q - win[i] ) > 0) printf("%s\n", "Yes");
    else printf("%s\n", "No");
  }
  return 0;
}
