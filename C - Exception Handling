#include <stdio.h>
#include <stdlib.h>

int cmp_longlong(const void *a, const void *b){
  long long x = *(long long*)a;
  long long y = *(long long*)b;
  
  if(x>y) return -1;
  else if(x==y) return 0;
  else if(x<y) return 1;
}

  long long a[200005];
  
int main(void){
  long long N, max=0, max_id;
  
  scanf("%lld", &N);
  
  for(int i=0; i<N; i++){
    scanf("%lld", &a[i]);
    if(a[i] > max){
      max = a[i];
      max_id = i;
    }
  }
  
  qsort(a, N, sizeof(long long), cmp_longlong);
  
  for(int i=0; i<N; i++){
    if(max_id != i) printf("%lld\n", max);
    else printf("%lld\n", a[1]);
  }
  return 0;
}
